{
  "name": "tu-table",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "utils/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "lib:build": "turbo run lib:build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare-publish": "npx changeset",
    "publish-packages": "turbo run build lint --filter @tjernstad-utvikling/* && changeset version && changeset publish"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.0"
  },
  "packageManager": "pnpm@7.28.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
